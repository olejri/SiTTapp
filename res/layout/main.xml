<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:weightSum="1" android:id="@string/mainlayout" android:background="@drawable/glos">
    <RelativeLayout android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_below="@string/topbar" android:layout_alignParentBottom="true" android:layout_alignParentRight="true" android:layout_alignParentLeft="true" android:id="@string/frontlayout">
        <Button android:id="@+id/sbutton" android:layout_height="80dip" android:layout_width="80dip" android:onClick="toTrainingNetwork" android:background="@drawable/s_network" android:clickable="true" android:layout_alignParentBottom="true" android:layout_toRightOf="@+id/lbutton"></Button>
        <Button android:id="@+id/lbutton" android:layout_height="80dip" android:layout_width="80dip" android:onClick="toLog" android:background="@drawable/s_log" android:clickable="true" android:layout_alignParentBottom="true" android:layout_toRightOf="@+id/tbutton"></Button>
        <Button android:id="@+id/gbutton" android:layout_height="80dip" android:layout_width="80dip" android:onClick="toJointTraining" android:background="@drawable/s_joint" android:clickable="true" android:layout_alignParentBottom="true" android:layout_alignParentLeft="true"></Button>
        <Button android:id="@+id/tbutton" android:layout_height="80dip" android:layout_width="80dip" android:onClick="toTrainingBook" android:background="@drawable/s_plan" android:clickable="true" android:layout_alignParentBottom="true" android:layout_toRightOf="@+id/gbutton"></Button>
        <Gallery android:minWidth="150dip" android:minHeight="150dip" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/window" android:layout_alignParentTop="true" android:layout_centerHorizontal="true" android:layout_marginTop="66dp"></Gallery>
    </RelativeLayout>
    <RelativeLayout android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@string/topbar" android:layout_alignParentTop="true" android:layout_alignParentLeft="true">
        <ImageButton android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/imageButton1" android:src="@drawable/sit" android:layout_alignParentTop="true" android:layout_centerHorizontal="true"></ImageButton>
        <RelativeLayout
            android:id="@+id/timewrap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/peoplewrap"
            android:layout_alignParentRight="true" android:clickable="true" android:onClick="toInfo">
            <TextView android:id="@+id/open" style="@style/regText" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Fredag" android:textColor="@color/black"></TextView>
            <DigitalClock android:textColor="@color/black" android:id="@+id/digitalClock1" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="DigitalClock" android:layout_below="@+id/open"></DigitalClock>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/peoplepicwrap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageView android:id="@+id/peoplepic" android:layout_width="wrap_content" android:src="@drawable/ic_menu_cc" android:layout_height="wrap_content"></ImageView>
        </RelativeLayout>
        <RelativeLayout android:id="@+id/peoplewrap" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_toLeftOf="@+id/imageButton1">
            <TextView android:id="@+id/people" style="@style/regText" android:layout_width="wrap_content" android:text="230" android:layout_height="wrap_content"></TextView>
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>
